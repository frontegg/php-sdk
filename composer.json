{
    "name": "frontegg/php-sdk",
    "description": "Frontegg SDK for PHP",
    "keywords": ["frontegg", "sdk"],
    "type": "library",
    "homepage": "https://github.com/frontegg/php-sdk",
    "license": "MIT License",
    "authors": [
        {
            "name": "Frontegg",
            "homepage": "https://github.com/frontegg/php-sdk/contributors"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "guzzlehttp/guzzle": "~7.0"
    },
    "suggest": {
        "guzzlehttp/guzzle": "Allows for implementation of the Guzzle HTTP client"
    },
    "autoload": {
        "psr-4": {
            "Frontegg\\": "src/Frontegg/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Frontegg\\Tests\\": "tests/"
        }
    }
}
